{
  "manifest_version": 2,
  "name": "Time Logger Browser Extension",
  "version": "1.0",
  "description": "Auto update of Timesheet by - Prajinkya Pimpalghare",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "icons": {
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
